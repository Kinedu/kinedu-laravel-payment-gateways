{
    "name": "kinedu/payment-gateways",
    "description": "Payment gateways for use in Kinedu projects.",
    "keywords": ["laravel", "kinedu", "billing"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ed Bentinck",
            "email": "ed@kinedu.com"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://bitbucket.org/kinedu/srpago-php"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "~5.3",
        "kinedu/srpago-php": "~1.3"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~5.0",
        "vlucas/phpdotenv": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Kinedu\\PaymentGateways\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kinedu\\PaymentGateways\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kinedu\\PaymentGateways\\PaymentGatewayServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
